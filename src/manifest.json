{
  "name": "todoist-shortcuts",
  "author": "Michael Sloan",
  "description": "Adds a bunch of keyboard shortcuts to Todoist. More info: https://github.com/mgsloan/todoist-shortcuts",
  "version": "9",
  "content_scripts": [
  	{
      "matches":
        [ "http://todoist.com/app*"
        , "https://todoist.com/app*"
        , "http://beta.todoist.com/app*"
        , "https://beta.todoist.com/app*"
        ],
      "js": ["inject.js"]
  	}
  ],
  "web_accessible_resources": [
  	"todoist-shortcuts.js"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "manifest_version": 2
}
